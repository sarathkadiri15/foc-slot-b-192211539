#include<stdio.h>
int main()
{
	int i,n, sum =0;
	printf("\n enter the number:");
	scanf("%d", &n);
	printf("\n odd numbers between 0 and %d are:",n);
	for(i=1;i<=n;i++)
	{
		if (i%2 !=0){
			printf("%d",i);
        	sum=sum+1;
			
		}
	}
	printf("\n \n the sum of odd numbers from 1 to %d=%d",n,sum);
	return 0;
	 
	
}
